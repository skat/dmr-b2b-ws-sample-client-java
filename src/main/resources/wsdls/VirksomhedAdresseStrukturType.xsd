                                    <xs:element minOccurs="0" ref="dmr:AlternativAdresseGyldigTil"/>
                                    <xs:element minOccurs="0" ref="dmr:LandKode"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
