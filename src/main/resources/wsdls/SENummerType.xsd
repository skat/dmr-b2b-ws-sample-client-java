<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified"
    targetNamespace="http://skat.dk/dmr/2007/05/31/" version="1.0"
    xmlns:dmr="http://skat.dk/dmr/2007/05/31/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:simpleType name="SENummerType">
        <xs:annotation>
            <xs:documentation>8-cifret nummer,  der entydigt
                identificerer en registreret virksomhed i SKAT.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:pattern value="[0-9]{8}"/>
            <xs:totalDigits value="8"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>
